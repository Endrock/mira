<script type="text/javascript">
    jQuery( document ).ready(function($) {

        var AMH_unit = "ng/mL";
        var TSH_unit = "uIU/mL";
        var FT4_unit = "ng/dL";
        var TESTE_unit = "%";
        var PROL_unit = "ng/mL";

        $('.lab-test-result-outer i, .lab-test-result-outer .lab-test-tooltip').hover(function() {
          $(this).parent('.top-box').addClass('open-tooltip');
        }, function() {
            $(this).parent('.top-box').removeClass('open-tooltip');
        }); 
        $("#my_reports a").click(function() {
          var report_id = $(this).data('id');
          $('html,body').animate({
            scrollTop: $('#'+report_id).offset().top - 130 }, 'slow');
        });
                              
         var user_id = "{{ customer.id }}";
        // var user_id = "6555428618392";

        var queryString = window.location.search;
        var urlParams = new URLSearchParams(queryString);
        var date = urlParams.get('date');
        console.log(date);

        var settings = { 
            "url": "https://www.miracare.com/wp-json/api/lab-test-api/get_report_data",
            "method": "POST",
            "timeout": 0,
            "headers": {
                "Content-Type": "application/json"
            },
            "data": JSON.stringify({
                "user_id": user_id,
                "date": date
            }),
        };
        
        $.ajax(settings).done(function (report_response) {
            $("#loader").hide();
            $(".lab-test-result-outer .bottom-box span").removeAttr('class');

            if (report_response.status == false) {
                console.log('no_data_found');
                $("#no_data_found").show();
                $("#data_found").hide();
            }
            else if(report_response.status == 'CANCELLED'){

                console.log('CANCELLED');
                $("#test_status_cancelled").show();
                $("#data_found").hide();
            }
            else{
                var report_data = report_response.report_data;
                console.log(report_data);

                var AMH = report_data.AMH;
                console.log(AMH);
                var TSH = report_data.TSH;
                console.log(TSH);
                var FT4 = report_data.FT4;
                console.log(FT4);
                var testosterone = report_data.testosterone;
                console.log(testosterone);
                var prolactin = report_data.prolactin;
                console.log(prolactin);
                var DOB = report_data.dob;
                console.log(DOB);

                $('#AMH_result').text(AMH);
                $('#AMH_result_chart').text(AMH + " " + AMH_unit);
                $('#TSH_result').text(TSH);
                $('#TSH_result_chart').text(TSH + " " + TSH_unit);
                $('#FT4_result').text(FT4);
                $('#FT4_result_chart').text(FT4 + " " + FT4_unit);
                $('#testosterone_result').text(testosterone);
                $('#testosterone_result_chart').text(testosterone + " " + TESTE_unit);
                $('#prolactin_result').text(prolactin);
                $('#prolactin_result_chart').text(prolactin + " " + PROL_unit);

                $('#AMH').data('result',AMH);
                $('#TSH').data('result',TSH);
                $('#fT4').data('result',FT4);
                $('#Free_Testosterone').data('result',testosterone);
                $('#Prolactine').data('result',prolactin);

                $('#previous_test_dates').html(report_response.all_reports_dates);
              
                var dob = new Date(DOB);
                var currentDate = new Date();
                var age = currentDate.getFullYear() - dob.getFullYear();

                // Adjust age based on the month and day
                var currentMonth = currentDate.getMonth() + 1;
                var birthMonth = dob.getMonth() + 1;
                var currentDay = currentDate.getDate();
                var birthDay = dob.getDate();

                if (currentMonth < birthMonth || (currentMonth === birthMonth && currentDay < birthDay)) {
                    age--;
                }
                console.log( age + " years");

                // Special case - min and max for AMH

                  if ( (age >= 36) && (age<=40) ){
                      $('#AMH').data('min',0.5);
                      $('#AMH').data('max',1.5);
                  } else if ( (age >= 41) && (age<=45) ){
                      $('#AMH').data('min',0.5);
                      $('#AMH').data('max',1.0);
                  }
    
                // End Special case - min and max for AMH

                var AMH_range = TSH_range = FT4_range = testosterone_range = Prolactin_range = "";

                // ==================== Range Start ==================== //
                // ===================================================== //

                // =============== AMH Range =============== //

                if ( (AMH > 0.5) && (AMH < 1.0) && (age<=35) ) {
                    AMH_range = "low_AMH_under_35";
                    $("span#AMH_badge,#AMH").addClass('low-bg');
                    $("span#AMH_badge").text('Low');
                }
                else if ( (AMH < 0.5) && (age >= 36) && (age<=40) ){
                    AMH_range = "low_AMH_36_to_40";
                    $("span#AMH_badge,#AMH").addClass('low-bg');
                    $("span#AMH_badge").text('Low');
                }
                else if ( (AMH < 0.5) && (age >= 41) && (age<=45) ){
                    AMH_range = "low_AMH_41_to_45";
                    $("span#AMH_badge,#AMH").addClass('low-bg');
                    $("span#AMH_badge").text('Low');
                }
                else if ( (AMH > 1.0) && (AMH <= 1.5) && (age<=35) ){
                    AMH_range = "low_normal_AMH_35";
                    $("span#AMH_badge,#AMH").addClass('low-bg');
                    $("span#AMH_badge").text('Low Normal');
                    $("span#AMH_badge").closest('.left').css('padding-left', '14px').css('padding-right', '14px');
                    
                }
                else if ( (AMH > 1.5) && (AMH <= 4) && (age<=35) ){
                    AMH_range = "normal_AMH_35";
                    $("span#AMH_badge,#AMH").addClass('narmal-bg');
                    $("span#AMH_badge").text('Normal');
                }
                else if ( (AMH > 0.5) && (AMH <= 1.5) && (age>=36) && (age<=40) ){
                    AMH_range = "normal_AMH_36_to_40";
                    $("span#AMH_badge,#AMH").addClass('narmal-bg');
                    $("span#AMH_badge").text('Normal');
                }
                else if ( (AMH > 0.5) && (AMH <= 1) && (age>=41) && (age<=45) ){
                    AMH_range = "normal_AMH_41_to_45";
                    $("span#AMH_badge,#AMH").addClass('narmal-bg');
                    $("span#AMH_badge").text('Normal');
                }
                else if ( (AMH > 4.0) && (age<=35) ){
                    AMH_range = "high_AMH_35";
                    $("span#AMH_badge,#AMH").addClass('heigh-bg');
                    $("span#AMH_badge").text('High');
                }
                else if ( (AMH >= 1.5) && (age>=36) && (age<=40) ){
                    AMH_range = "high_AMH__36_to_40";
                    $("span#AMH_badge,#AMH").addClass('heigh-bg');
                    $("span#AMH_badge").text('High');
                }
                else if ( (AMH > 1.0) && (age>=41) && (age<=45) ){
                    AMH_range = "high_AMH__41_to_45";
                    $("span#AMH_badge,#AMH").addClass('heigh-bg');
                    $("span#AMH_badge").text('High');
                }
                else{
                    AMH_range = "AMH Not Range";
                }
                console.log(AMH_range);

                // =============== TSH Range =============== //
                if ( TSH < 0.45) {
                    TSH_range = "low_TSH";
                    $("span#TSH_badge,#TSH").addClass('low-bg');
                    $("span#TSH_badge").text('Low');
                    //$("#AMH").addClass('low');
                }
                else if ( (TSH >= 0.45) && (TSH <= 5.33) ){
                    TSH_range = "normal_TSH";
                    $("span#TSH_badge,#TSH").addClass('narmal-bg');
                    $("span#TSH_badge").text('Normal');
                    //$("#AMH").addClass('normal');
                }
                else if ( TSH > 5.33 ){
                    TSH_range = "high_TSH";
                    $("span#TSH_badge,#TSH").addClass('heigh-bg');
                    $("span#TSH_badge").text('High');
                    //$("#AMH").addClass('high');
                }
                else{
                    TSH_range = "TSH Not Range";
                }
                console.log(TSH_range);

                // =============== FT4 Range =============== //
                if ( FT4 < 0.61) {
                    FT4_range = "low_FT4";
                    $("span#FT4_badge,#fT4").addClass('low-bg');
                    $("span#FT4_badge").text('Low');
                }
                else if ( (FT4 >= 0.61) && (FT4 <= 1.12) ){
                    FT4_range = "normal_FT4";
                    $("span#FT4_badge,#fT4").addClass('narmal-bg');
                    $("span#FT4_badge").text('Normal');
                }
                else if ( FT4 > 1.12 ){
                    FT4_range = "high_FT4";
                    $("span#FT4_badge,#fT4").addClass('heigh-bg');
                    $("span#FT4_badge").text('High');
                }
                else{
                    FT4_range = "FT4 Not Range";
                }
                console.log(FT4_range);

                // =============== Testosterone Range =============== //
                if ( testosterone < 24.30) {
                    testosterone_range = "low_testosterone";
                    $("span#testo_badge,#Free_Testosterone").addClass('low-bg');
                    $("span#testo_badge").text('Low');
                }
                else if ( (testosterone >= 24.30) && (testosterone <= 110.20) ){
                    testosterone_range = "normal_testosterone";
                    $("span#testo_badge,#Free_Testosterone").addClass('narmal-bg');
                  $("span#testo_badge").text('Normal');
                }
                else if ( testosterone > 110.20 ){
                    testosterone_range = "high_testosterone";
                    $("span#testo_badge,#Free_Testosterone").addClass('heigh-bg');
                    $("span#testo_badge").text('High');
                }
                else{
                    testosterone_range = "Testosterone Not Range";
                }
                console.log(testosterone_range);

                // =============== Prolactin Range =============== //

                if(age < 50){
                  $(".multibar_small#Prolactine").data("min", 3.34);
                  $(".multibar_small#Prolactine").data("max", 26.72);
